{
    "parser": "babel-eslint",
    "extends": ["airbnb", "plugin:jest/recommended","plugin:jsx-a11y/recommended"],
    "env": {
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "rules": {
      "no-bitwise":0,
      "jsx-a11y/no-static-element-interactions": 0,
      "react/require-default-props": 0,
      "react/jsx-no-bind": 0,
      "react/sort-comp": 0,
      "comma-dangle": [2, "never"],
      "no-shadow": 0,
      "react/jsx-closing-tag-location":0,
      "import/prefer-default-export":0,
      "jsx-a11y/click-events-have-key-events":0,
      "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
      "linebreak-style": 0
    },
    "globals": {
      "describe": true,
      "it": true,
      "expect": true
    },
    "plugins": [
      "react",
      "jest",
      "jsx-a11y"
    ]
  }
  